@using Entities.Concrete
@model TisJobCode
@{
    ViewBag.Title = "TisJobCodeAdd";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Tis Meslek Kolu Ekle</h3>
<form method="post">
    <div class="box box-primary">
        <div class="box-header with-border">
            <table id="example2" class="table table-bordered table-hover dataTable" role="grid" aria-describedby="example2_info">
                <thead>

                </thead>
                <tbody>
                    <tr role="row" class="odd">
                        <td class="sorting_1"><b>Meslek Kodu</b></td>
                        <td>
                            @Html.TextBoxFor(x => x.TisMeslekKodId, null, new { @class = "form-control" })
                        </td>
                        <td><b>Meslek Kolu</b></td>
                        <td>
                            @Html.TextBoxFor(x => x.TisMeslekAdi, null, new { @class = "form-control" })
                        </td>
                    </tr>
            </table>
            <button class="btn btn-success">Ekle</button>
        </div>
    </div>
</form>


