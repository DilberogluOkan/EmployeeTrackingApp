
@using Entities.Concrete
@model PrivateStatus
@{
    ViewBag.Title = "PrivateStatusAdd";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Özel Durum Ekle</h3>
@using (Html.BeginForm("PrivateStatusAdd", "PrivateStatus", FormMethod.Post))
{
    <div class="box box-primary">
        <div class="box-header with-border">
            <table id="example2" class="table table-bordered table-hover dataTable" role="grid" aria-describedby="example2_info">
                <thead>

                </thead>
                <tbody>
                    <tr role="row" class="odd">
                        <td class="sorting_1"><b>Özel Durum</b></td>
                        <td>
                            @Html.HiddenFor(x => x.OzelDurumId)
                            @Html.TextBoxFor(x => x.OzelDurum, null, new { @class = "form-control" })
                        </td>
                    </tr>
            </table>
            <button class="btn btn-warning">Ekle</button>
        </div>
    </div>
}


